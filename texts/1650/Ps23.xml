                </l>
                <l>
                <seg type="syl" n="1">shall</seg>
                <seg type="syl" n="2">sure-</seg>
                <seg type="syl" n="3">ly</seg>
                <seg type="syl" n="4">fol-</seg>
                <seg type="syl" n="5">low</seg>
                <seg type="syl" n="6">me:</seg>
                </l>
                <l>
                <seg type="syl" n="1">And</seg>
                <seg type="syl" n="2">in</seg>
                <seg type="syl" n="3">GOD's</seg>
                <seg type="syl" n="4">house</seg>
                <seg type="syl" n="5">for</seg>
                <seg type="syl" n="6">ev-</seg>
                <seg type="syl" n="7">er-</seg>
                <seg type="syl" n="8">more</seg>
                </l>
                <l>
                <seg type="syl" n="1">my</seg>
                <seg type="syl" n="2">dwell-</seg>
                <seg type="syl" n="3">ing-</seg>
                <seg type="syl" n="4">place</seg>
                <seg type="syl" n="5">shall</seg>
                <seg type="syl" n="6">be.</seg>

                <seg type="syl" n="6">with</seg>
                <seg type="syl" n="7">con-</seg>
                <seg type="syl" n="8">tempt</seg>
                </l>
                <l>
                <seg type="syl" n="1">ex-</seg>
                <seg type="syl" n="2">ceed-</seg>
                <seg type="syl" n="3">ing-</seg>
                <seg type="syl" n="4">ly</seg>
                <seg type="syl" n="5">are</seg>
                <seg type="syl" n="6">we.</seg>
                </l>
                </lg>
                <lg type="verse" n="4">
                <l>
                <seg type="syl" n="1">4. Our</seg>
                <seg type="syl" n="2">soul</seg>
                <seg type="syl" n="3">is</seg>
                <seg type="syl" n="4">filled</seg>
                <seg type="syl" n="5">with</seg>
                <seg type="syl" n="6">scorn</seg>
                <seg type="syl" n="7">of</seg>
                <seg type="syl" n="8">those</seg>
                </l>
                <l>
                <seg type="syl" n="1">that</seg>
                <seg type="syl" n="2">at</seg>
                <seg type="syl" n="3">their</seg>
                <seg type="syl" n="4">ease</seg>
                <seg type="syl" n="5">a-</seg>
                <seg type="syl" n="6">bide,</seg>
                </l>
                <l>
                <seg type="syl" n="1">And</seg>
                <seg type="syl" n="2">with</seg>
                <seg type="syl" n="3">the</seg>
                <seg type="syl" n="4">in-</seg>
                <seg type="syl" n="5">so-</seg>
                <seg type="syl" n="6">lent</seg>
                <seg type="syl" n="7">con-</seg>
                <seg type="syl" n="8">tempt</seg>
                </l>
                <l>
                <seg type="syl" n="1">of</seg>
                <seg type="syl" n="2">those</seg>
                <seg type="syl" n="3">that</seg>
                <seg type="syl" n="4">swell</seg>
                <seg type="syl" n="5">in</seg>
                <seg type="syl" n="6">pride.</seg>
                </l>
            </lg>

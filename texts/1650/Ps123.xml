<text>
    <body>
        <div type="poem" met="8.6.8.6.">        
            <lg type="verse" n="1-2a">
                <l>
                <seg type="syl" n="1">1. O</seg>
                <seg type="syl" n="2">thou</seg>
                <seg type="syl" n="3">that</seg>
                <seg type="syl" n="4">dwel-</seg>
                <seg type="syl" n="5">lest</seg>
                <seg type="syl" n="6">in</seg>
                <seg type="syl" n="7">the</seg>
                <seg type="syl" n="8">heav'ns,</seg>
                </l>
                <l>
                <seg type="syl" n="1">I</seg>
                <seg type="syl" n="2">lift</seg>
                <seg type="syl" n="3">mine</seg>
                <seg type="syl" n="4">eyes</seg>
                <seg type="syl" n="5">to</seg>
                <seg type="syl" n="6">thee.</seg>
                </l>
                <l>
                <seg type="syl" n="1">2. Be-</seg>
                <seg type="syl" n="2">hold,</seg>
                <seg type="syl" n="3">as</seg>
                <seg type="syl" n="4">ser-</seg>
                <seg type="syl" n="5">vants'</seg>
                <seg type="syl" n="6">eyes</seg>
                <seg type="syl" n="7">do</seg>
                <seg type="syl" n="8">look</seg>
                </l>
                <l>
                <seg type="syl" n="1">their</seg>
                <seg type="syl" n="2">mas-</seg>
                <seg type="syl" n="3">ters'</seg>
                <seg type="syl" n="4">hand</seg>
                <seg type="syl" n="5">to</seg>
                <seg type="syl" n="6">see,</seg>
                </l>
                </lg>
                <lg type="verse" n="2b">
                <l>
                <seg type="syl" n="1">As</seg>
                <seg type="syl" n="2">hand-</seg>
                <seg type="syl" n="3">maid's</seg>
                <seg type="syl" n="4">eyes</seg>
                <seg type="syl" n="5">her</seg>
                <seg type="syl" n="6">mis-</seg>
                <seg type="syl" n="7">tress'</seg>
                <seg type="syl" n="8">hand,</seg>
                </l>
                <l>
                <seg type="syl" n="1">so</seg>
                <seg type="syl" n="2">do</seg>
                <seg type="syl" n="3">our</seg>
                <seg type="syl" n="4">eyes</seg>
                <seg type="syl" n="5">at-</seg>
                <seg type="syl" n="6">tend</seg>
                </l>
                <l>
                <seg type="syl" n="1">Up-</seg>
                <seg type="syl" n="2">on</seg>
                <seg type="syl" n="3">the</seg>
                <seg type="syl" n="4">LORD</seg>
                <seg type="syl" n="5">our</seg>
                <seg type="syl" n="6">God,</seg>
                <seg type="syl" n="7">un-</seg>
                <seg type="syl" n="8">til</seg>
                </l>
                <l>
                <seg type="syl" n="1">to</seg>
                <seg type="syl" n="2">us</seg>
                <seg type="syl" n="3">he</seg>
                <seg type="syl" n="4">mer-</seg>
                <seg type="syl" n="5">cy</seg>
                <seg type="syl" n="6">send.</seg>
                </l>
                </lg>
                <lg type="verse" n="3">
                <l>
                <seg type="syl" n="1">3. O</seg>
                <seg type="syl" n="2">LORD,</seg>
                <seg type="syl" n="3">be</seg>
                <seg type="syl" n="4">gra-</seg>
                <seg type="syl" n="5">ci-</seg>
                <seg type="syl" n="6">ous</seg>
                <seg type="syl" n="7">to</seg>
                <seg type="syl" n="8">us,</seg>
                </l>
                <l>
                <seg type="syl" n="1">un-</seg>
                <seg type="syl" n="2">to</seg>
                <seg type="syl" n="3">us</seg>
                <seg type="syl" n="4">gra-</seg>
                <seg type="syl" n="5">cious</seg>
                <seg type="syl" n="6">be,</seg>
                </l>
                <l>
                <seg type="syl" n="1">Be-</seg>
                <seg type="syl" n="2">cause</seg>
                <seg type="syl" n="3">re-</seg>
                <seg type="syl" n="4">plen-</seg>

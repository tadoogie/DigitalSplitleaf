<text>
    <body>
        <div type="poem" met="8.6.8.6.">        
            <lg type="verse" n="1-2">
                <l>
                <seg type="syl" n="1">1. In</seg>
                <seg type="syl" n="2">my</seg>
                <seg type="syl" n="3">dis-</seg>
                <seg type="syl" n="4">tress</seg>
                <seg type="syl" n="5">to</seg>
                <seg type="syl" n="6">GOD</seg>
                <seg type="syl" n="7">I</seg>
                <seg type="syl" n="8">cried,</seg>
                </l>
                <l>
                <seg type="syl" n="1">and</seg>
                <seg type="syl" n="2">he</seg>
                <seg type="syl" n="3">gave</seg>
                <seg type="syl" n="4">ear</seg>
                <seg type="syl" n="5">to</seg>
                <seg type="syl" n="6">me.</seg>
                </l>
                <l>
                <seg type="syl" n="1">2. From</seg>
                <seg type="syl" n="2">ly-</seg>
                <seg type="syl" n="3">ing</seg>
                <seg type="syl" n="4">lips,</seg>
                <seg type="syl" n="5">and</seg>
                <seg type="syl" n="6">guile-</seg>
                <seg type="syl" n="7">ful</seg>
                <seg type="syl" n="8">tongue,</seg>
                </l>
                <l>
                <seg type="syl" n="1">O</seg>
                <seg type="syl" n="2">LORD,</seg>
                <seg type="syl" n="3">my</seg>
                <seg type="syl" n="4">soul</seg>
                <seg type="syl" n="5">set</seg>
                <seg type="syl" n="6">free.</seg>
                </l>
                </lg>
                <lg type="verse" n="3-4">
                <l>
                <seg type="syl" n="1">3. What</seg>
                <seg type="syl" n="2">shall</seg>
                <seg type="syl" n="3">be</seg>
                <seg type="syl" n="4">giv'n</seg>
                <seg type="syl" n="5">thee?</seg>
                <seg type="syl" n="6">or</seg>
                <seg type="syl" n="7">what</seg>
                <seg type="syl" n="8">shall</seg>
                </l>
                <l>
                <seg type="syl" n="1">be</seg>
                <seg type="syl" n="2">done</seg>
                <seg type="syl" n="3">to</seg>
                <seg type="syl" n="4">thee,</seg>
                <seg type="syl" n="5">false</seg>
                <seg type="syl" n="6">tongue?</seg>
                </l>
                <l>
                <seg type="syl" n="1">4. Ev'n</seg>
                <seg type="syl" n="2">bur-</seg>
                <seg type="syl" n="3">ning</seg>
                <seg type="syl" n="4">coals</seg>
                <seg type="syl" n="5">of</seg>
                <seg type="syl" n="6">ju-</seg>
                <seg type="syl" n="7">ni-</seg>
                <seg type="syl" n="8">per,</seg>
                </l>
                <l>
                <seg type="syl" n="1">sharp</seg>
                <seg type="syl" n="2">ar-</seg>
                <seg type="syl" n="3">rows</seg>
                <seg type="syl" n="4">of</seg>
                <seg type="syl" n="5">the</seg>
                <seg type="syl" n="6">strong.</seg>
                </l>
                </lg>
                <lg type="verse" n="5">
                <l>
                <seg type="syl" n="1">5. Woe's</seg>
                <seg type="syl" n="2">me</seg>
                <seg type="syl" n="3">that</seg>
                <seg type="syl" n="4">I</seg>
                <seg type="syl" n="5">in</seg>
                <seg type="syl" n="6">Me-</seg>
                <seg type="syl" n="7">sech</seg>
                <seg type="syl" n="8">am</seg>
                </l>
                <l>
                <seg type="syl" n="1">a</seg>
                <seg type="syl" n="2">so-</seg>
                <seg type="syl" n="3">jour-</seg>
                <seg type="syl" n="4">ner</seg>
                <seg type="syl" n="5">so</seg>
                <seg type="syl" n="6">long,</seg>
                </l>
                <l>
                <seg type="syl" n="1">That</seg>
                <seg type="syl" n="2">I</seg>
                <seg type="syl" n="3">in</seg>
                <seg type="syl" n="4">tab-</seg>
                <seg type="syl" n="5">er-</seg>
                <seg type="syl" n="6">na-</seg>
                <seg type="syl" n="7">cles</seg>
                <seg type="syl" n="8">dwell</seg>
                </l>
                <l>
                <seg type="syl" n="1">to</seg>
                <seg type="syl" n="2">Ke-</seg>
                <seg type="syl" n="3">dar</seg>
                <seg type="syl" n="4">that</seg>
                <seg type="syl" n="5">be-</seg>
                <seg type="syl" n="6">long.</seg>
                </l>
                </lg>
                <lg type="verse" n="6-7">
                <l>
                <seg type="syl" n="1">6. My</seg>
                <seg type="syl" n="2">soul</seg>
                <seg type="syl" n="3">with</seg>
                <seg type="syl" n="4">him</seg>
                <seg type="syl" n="5">that</seg>
                <seg type="syl" n="6">hat-</seg>
                <seg type="syl" n="7">eth</seg>
                <seg type="syl" n="8">peace</seg>
                </l>
                <l>
                <seg type="syl" n="1">hath</seg>
                <seg type="syl" n="2">long</seg>
                <seg type="syl" n="3">a</seg>
                <seg type="syl" n="4">dwel-</seg>
                <seg type="syl" n="5">ler</seg>
                <seg type="syl" n="6">been.</seg>
                </l>
                <l>
                <seg type="syl" n="1">7. I</seg>
                <seg type="syl" n="2">am</seg>
                <seg type="syl" n="3">for</seg>
                <seg type="syl" n="4">peace,</seg>
                <seg type="syl" n="5">but</seg>
                <seg type="syl" n="6">when</seg>
                <seg type="syl" n="7">I</seg>
                <seg type="syl" n="8">speak,</seg>
                </l>
                <l>
                <seg type="syl" n="1">for</seg>
                <seg type="syl" n="2">bat-</seg>
                <seg type="syl" n="3">tle</seg>
                <seg type="syl" n="4">they</seg>
                <seg type="syl" n="5">are</seg>
                <seg type="syl" n="6">keen.</seg>
                </l>
            </lg>
        </div>

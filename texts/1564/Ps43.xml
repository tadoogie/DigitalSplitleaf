                    <seg type="syl" n="2">frettest</seg>
                    <seg type="syl" n="3">thus</seg>
                    <seg type="syl" n="4">in</seg>
                    <seg type="syl" n="5">my</seg>
                    <seg type="syl" n="6">breast?</seg>
                </l>

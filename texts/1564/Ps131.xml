<text>
    <body>
        <div type="poem" met="8.6.8.6.">
            <lg type="verse" n="1">
                <l>
                    <seg type="syl" n="1">1. Lord</seg>
                    <seg type="syl" n="2">I</seg>
                    <seg type="syl" n="3">am</seg>
                    <seg type="syl" n="4">not</seg>
                    <seg type="syl" n="5">puffed</seg>
                    <seg type="syl" n="6">up</seg>
                    <seg type="syl" n="7">in</seg>
                    <seg type="syl" n="8">mind,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">I</seg>
                    <seg type="syl" n="2">have</seg>
                    <seg type="syl" n="3">no</seg>
                    <seg type="syl" n="4">scorn-</seg>
                    <seg type="syl" n="5">ful</seg>
                    <seg type="syl" n="6">eye:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">I</seg>
                    <seg type="syl" n="2">do</seg>
                    <seg type="syl" n="3">not</seg>
                    <seg type="syl" n="4">ex-</seg>
                    <seg type="syl" n="5">er-</seg>
                    <seg type="syl" n="6">cise</seg>
                    <seg type="syl" n="7">my-</seg>
                    <seg type="syl" n="8">self</seg>
                </l>
                <l>
                    <seg type="syl" n="1">in</seg>
                    <seg type="syl" n="2">things</seg>
                    <seg type="syl" n="3">that</seg>
                    <seg type="syl" n="4">be</seg>
                    <seg type="syl" n="5">too</seg>
                    <seg type="syl" n="6">high.</seg>
                </l>
            </lg>
            <lg type="verse" n="2">
                <l>
                    <seg type="syl" n="1">2. But</seg>
                    <seg type="syl" n="2">as</seg>
                    <seg type="syl" n="3">a</seg>
                    <seg type="syl" n="4">child</seg>
                    <seg type="syl" n="5">that</seg>
                    <seg type="syl" n="6">wean-</seg>
                    <seg type="syl" n="7">ed</seg>
                    <seg type="syl" n="8">is,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">even</seg>
                    <seg type="syl" n="2">from</seg>
                    <seg type="syl" n="3">his</seg>
                    <seg type="syl" n="4">moth-</seg>
                    <seg type="syl" n="5">er's</seg>
                    <seg type="syl" n="6">breast:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">So</seg>
                    <seg type="syl" n="2">have</seg>
                    <seg type="syl" n="3">I</seg>
                    <seg type="syl" n="4">Lord</seg>
                    <seg type="syl" n="5">be-</seg>
                    <seg type="syl" n="6">haved</seg>
                    <seg type="syl" n="7">my-</seg>
                    <seg type="syl" n="8">self,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">in</seg>
                    <seg type="syl" n="2">si-</seg>
                    <seg type="syl" n="3">lence</seg>
                    <seg type="syl" n="4">and</seg>
                    <seg type="syl" n="5">in</seg>
                    <seg type="syl" n="6">rest.</seg>
                </l>
            </lg>
            <lg type="verse" n="3">
                <l>
                    <seg type="syl" n="1">3. O</seg>
                    <seg type="syl" n="2">Is-</seg>
                    <seg type="syl" n="3">ra-</seg>
                    <seg type="syl" n="4">el,</seg>
                    <seg type="syl" n="5">trust</seg>
                    <seg type="syl" n="6">in</seg>
                    <seg type="syl" n="7">the</seg>
                    <seg type="syl" n="8">Lord,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">let</seg>
                    <seg type="syl" n="2">him</seg>
                    <seg type="syl" n="3">be</seg>
                    <seg type="syl" n="4">all</seg>
                    <seg type="syl" n="5">thy</seg>
                    <seg type="syl" n="6">stay:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">From</seg>
                    <seg type="syl" n="2">this</seg>
                    <seg type="syl" n="3">time</seg>
                    <seg type="syl" n="4">forth</seg>
                    <seg type="syl" n="5">for-</seg>
                    <seg type="syl" n="6">ev-</seg>
                    <seg type="syl" n="7">er-</seg>
                    <seg type="syl" n="8">more,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">from</seg>
                    <seg type="syl" n="2">age</seg>
                    <seg type="syl" n="3">to</seg>
                    <seg type="syl" n="4">age</seg>
                    <seg type="syl" n="5">I</seg>
                    <seg type="syl" n="6">say.</seg>
                </l>
            </lg>

                    <seg type="syl" n="2">hast</seg>
                    <seg type="syl" n="3">mine</seg>
                    <seg type="syl" n="4">head</seg>
                    <seg type="syl" n="5">with</seg>
                    <seg type="syl" n="6">balm</seg>
                    <seg type="syl" n="7">re-</seg>
                    <seg type="syl" n="8">freshed,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">my</seg>
                    <seg type="syl" n="2">cup</seg>
                    <seg type="syl" n="3">doth</seg>
                    <seg type="syl" n="4">o-</seg>
                    <seg type="syl" n="5">ver-</seg>
                    <seg type="syl" n="6">flow.</seg>
                </l>
            </lg>
            <lg type="verse" n="6">
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">fi-</seg>
                    <seg type="syl" n="3">nal-</seg>
                    <seg type="syl" n="4">ly</seg>
                    <seg type="syl" n="5">while</seg>
                    <seg type="syl" n="6">breath</seg>
                    <seg type="syl" n="7">doth</seg>

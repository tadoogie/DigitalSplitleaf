<text>
    <body>
        <div type="poem" met="8.8.8.8.">
             <lg type="verse" n="1-2">
                <l>
                    <seg type="syl" n="1">1. All</seg>
                    <seg type="syl" n="2">peo-</seg>
                    <seg type="syl" n="3">ple</seg>
                    <seg type="syl" n="4">that</seg>
                    <seg type="syl" n="5">on</seg>
                    <seg type="syl" n="6">earth</seg>
                    <seg type="syl" n="7">do</seg>
                    <seg type="syl" n="8">dwell,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">sing</seg>
                    <seg type="syl" n="2">to</seg>
                    <seg type="syl" n="3">the</seg>
                    <seg type="syl" n="4">Lord</seg>
                    <seg type="syl" n="5">with</seg>
                    <seg type="syl" n="6">cheer-</seg>
                    <seg type="syl" n="7">ful</seg>
                    <seg type="syl" n="8">voice:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">2. Him</seg>
                    <seg type="syl" n="2">serve</seg>
                    <seg type="syl" n="3">with</seg>
                    <seg type="syl" n="4">fear,</seg>
                    <seg type="syl" n="5">his</seg>
                    <seg type="syl" n="6">praise</seg>
                    <seg type="syl" n="7">forth</seg>
                    <seg type="syl" n="8">tell:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Come</seg>
                    <seg type="syl" n="2">ye</seg>
                    <seg type="syl" n="3">be-</seg>
                    <seg type="syl" n="4">fore</seg>
                    <seg type="syl" n="5">him</seg>
                    <seg type="syl" n="6">and</seg>
                    <seg type="syl" n="7">re-</seg>
                    <seg type="syl" n="8">joice.</seg>
                </l>
            </lg>
            <lg type="verse" n="3">
                <l>
                    <seg type="syl" n="1">3. The</seg>
                    <seg type="syl" n="2">Lord,</seg>
                    <seg type="syl" n="3">ye</seg>
                    <seg type="syl" n="4">know,</seg>
                    <seg type="syl" n="5">is</seg>
                    <seg type="syl" n="6">God</seg>
                    <seg type="syl" n="7">in-</seg>
                    <seg type="syl" n="8">deed:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">with-</seg>
                    <seg type="syl" n="2">out</seg>
                    <seg type="syl" n="3">our</seg>
                    <seg type="syl" n="4">aid,</seg>
                    <seg type="syl" n="5">he</seg>
                    <seg type="syl" n="6">did</seg>
                    <seg type="syl" n="7">us</seg>
                    <seg type="syl" n="8">make:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">We</seg>
                    <seg type="syl" n="2">are</seg>
                    <seg type="syl" n="3">his</seg>
                    <seg type="syl" n="4">folk:</seg>
                    <seg type="syl" n="5">he</seg>
                    <seg type="syl" n="6">doth</seg>
                    <seg type="syl" n="7">us</seg>
                    <seg type="syl" n="8">feed,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">for</seg>
                    <seg type="syl" n="3">his</seg>
                    <seg type="syl" n="4">sheep</seg>
                    <seg type="syl" n="5">he</seg>
                    <seg type="syl" n="6">doth</seg>
                    <seg type="syl" n="7">us</seg>
                    <seg type="syl" n="8">take.</seg>
                </l>
            </lg>
            <lg type="verse" n="4">
                <l>
                    <seg type="syl" n="1">4. Oh,</seg>
                    <seg type="syl" n="2">en-</seg>
                    <seg type="syl" n="3">ter</seg>
                    <seg type="syl" n="4">then</seg>
                    <seg type="syl" n="5">his</seg>
                    <seg type="syl" n="6">gates</seg>
                    <seg type="syl" n="7">with</seg>
                    <seg type="syl" n="8">praise:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Ap-</seg>
                    <seg type="syl" n="2">proach</seg>
                    <seg type="syl" n="3">with</seg>
                    <seg type="syl" n="4">joy</seg>
                    <seg type="syl" n="5">his</seg>
                    <seg type="syl" n="6">courts</seg>
                    <seg type="syl" n="7">un-</seg>
                    <seg type="syl" n="8">to:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Praise,</seg>
                    <seg type="syl" n="2">laud,</seg>
                    <seg type="syl" n="3">and</seg>
                    <seg type="syl" n="4">bless</seg>
                    <seg type="syl" n="5">his</seg>
                    <seg type="syl" n="6">name</seg>
                    <seg type="syl" n="7">al-</seg>
                    <seg type="syl" n="8">ways,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">For</seg>
                    <seg type="syl" n="2">it</seg>
                    <seg type="syl" n="3">is</seg>
                    <seg type="syl" n="4">seem-</seg>
                    <seg type="syl" n="5">ly</seg>
                    <seg type="syl" n="6">so</seg>
                    <seg type="syl" n="7">to</seg>
                    <seg type="syl" n="8">do.</seg>
                </l>
            </lg>
            <lg type="verse" n="5">
                <l>
                    <seg type="syl" n="1">5. For</seg>
                    <seg type="syl" n="2">why?</seg>
                    <seg type="syl" n="3">The</seg>
                    <seg type="syl" n="4">Lord</seg>
                    <seg type="syl" n="5">our</seg>
                    <seg type="syl" n="6">God</seg>
                    <seg type="syl" n="7">is</seg>
                    <seg type="syl" n="8">good:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">His</seg>
                    <seg type="syl" n="2">mer-</seg>
                    <seg type="syl" n="3">cy</seg>
                    <seg type="syl" n="4">is</seg>
                    <seg type="syl" n="5">for-</seg>
                    <seg type="syl" n="6">ev-</seg>
                    <seg type="syl" n="7">er</seg>
                    <seg type="syl" n="8">sure:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">His</seg>
                    <seg type="syl" n="2">truth</seg>
                    <seg type="syl" n="3">at</seg>
                    <seg type="syl" n="4">all</seg>
                    <seg type="syl" n="5">times</seg>
                    <seg type="syl" n="6">firm-</seg>
                    <seg type="syl" n="7">ly</seg>
                    <seg type="syl" n="8">stood,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">shall</seg>
                    <seg type="syl" n="3">from</seg>
                    <seg type="syl" n="4">age</seg>
                    <seg type="syl" n="5">to</seg>
                    <seg type="syl" n="6">age</seg>
                    <seg type="syl" n="7">en-</seg>
                    <seg type="syl" n="8">dure.</seg>
                </l>
            </lg>
        </div>
    </body>
</text>

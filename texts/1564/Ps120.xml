<text>
    <body>
        <div type="poem" met="6.6.6.6.6.6.">
             <lg type="verse" n="1-2">
                <l>
                    <seg type="syl" n="1">1. In</seg>
                    <seg type="syl" n="2">trou-</seg>
                    <seg type="syl" n="3">ble</seg>
                    <seg type="syl" n="4">and</seg>
                    <seg type="syl" n="5">in</seg>
                    <seg type="syl" n="6">thrall,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Un-</seg>
                    <seg type="syl" n="2">to</seg>
                    <seg type="syl" n="3">the</seg>
                    <seg type="syl" n="4">Lord</seg>
                    <seg type="syl" n="5">I</seg>
                    <seg type="syl" n="6">call,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">he</seg>
                    <seg type="syl" n="3">doth</seg>
                    <seg type="syl" n="4">me</seg>
                    <seg type="syl" n="5">com-</seg>
                    <seg type="syl" n="6">fort:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">2. De-</seg>
                    <seg type="syl" n="2">liv-</seg>
                    <seg type="syl" n="3">er</seg>
                    <seg type="syl" n="4">me,</seg>
                    <seg type="syl" n="5">I</seg>
                    <seg type="syl" n="6">say,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">From</seg>
                    <seg type="syl" n="2">li-</seg>
                    <seg type="syl" n="3">ars'</seg>
                    <seg type="syl" n="4">lips</seg>
                    <seg type="syl" n="5">al-</seg>
                    <seg type="syl" n="6">way,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">tongues</seg>
                    <seg type="syl" n="3">of</seg>
                    <seg type="syl" n="4">false</seg>
                    <seg type="syl" n="5">re-</seg>
                    <seg type="syl" n="6">port.</seg>
                </l>
            </lg>
            <lg type="verse" n="3-4">
                <l>
                    <seg type="syl" n="1">3. What</seg>
                    <seg type="syl" n="2">van-</seg>
                    <seg type="syl" n="3">tage</seg>
                    <seg type="syl" n="4">or</seg>
                    <seg type="syl" n="5">what</seg>
                    <seg type="syl" n="6">thing</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Gettest</seg>
                    <seg type="syl" n="2">thou</seg>
                    <seg type="syl" n="3">thus</seg>
                    <seg type="syl" n="4">for</seg>
                    <seg type="syl" n="5">to</seg>
                    <seg type="syl" n="6">sting,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Thou</seg>
                    <seg type="syl" n="2">false</seg>
                    <seg type="syl" n="3">and</seg>
                    <seg type="syl" n="4">flat-</seg>
                    <seg type="syl" n="5">tering</seg>
                    <seg type="syl" n="6">liar?</seg>
                </l>
                <l>
                    <seg type="syl" n="1">4. Thy</seg>
                    <seg type="syl" n="2">tongue</seg>
                    <seg type="syl" n="3">doth</seg>
                    <seg type="syl" n="4">hurt</seg>
                    <seg type="syl" n="5">I</seg>
                    <seg type="syl" n="6">wean</seg>
                </l>
                <l>
                    <seg type="syl" n="1">No</seg>
                    <seg type="syl" n="2">less</seg>
                    <seg type="syl" n="3">than</seg>
                    <seg type="syl" n="4">ar-</seg>
                    <seg type="syl" n="5">rows</seg>
                    <seg type="syl" n="6">keen,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Of</seg>
                    <seg type="syl" n="2">hot</seg>
                    <seg type="syl" n="3">con-</seg>
                    <seg type="syl" n="4">sum-</seg>
                    <seg type="syl" n="5">ing</seg>
                    <seg type="syl" n="6">fire.</seg>
                </l>
            </lg>
            <lg type="verse" n="5">
                <l>
                    <seg type="syl" n="1">5. A-</seg>
                    <seg type="syl" n="2">las</seg>
                    <seg type="syl" n="3">too</seg>
                    <seg type="syl" n="4">long</seg>
                    <seg type="syl" n="5">I</seg>
                    <seg type="syl" n="6">slack</seg>
                </l>
                <l>
                    <seg type="syl" n="1">With-</seg>
                    <seg type="syl" n="2">in</seg>
                    <seg type="syl" n="3">these</seg>
                    <seg type="syl" n="4">tents</seg>
                    <seg type="syl" n="5">so</seg>
                    <seg type="syl" n="6">black,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">(Which</seg>
                    <seg type="syl" n="2">Ke-</seg>
                    <seg type="syl" n="3">dar's</seg>
                    <seg type="syl" n="4">are</seg>
                    <seg type="syl" n="5">by</seg>
                    <seg type="syl" n="6">name:)</seg>
                </l>
                <l>
                    <seg type="syl" n="1">By</seg>
                    <seg type="syl" n="2">whom</seg>
                    <seg type="syl" n="3">the</seg>
                    <seg type="syl" n="4">folk</seg>
                    <seg type="syl" n="5">e-</seg>
                    <seg type="syl" n="6">lect,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">all</seg>
                    <seg type="syl" n="3">of</seg>
                    <seg type="syl" n="4">Is-</seg>
                    <seg type="syl" n="5">aac's</seg>
                    <seg type="syl" n="6">sect,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Are</seg>
                    <seg type="syl" n="2">put</seg>
                    <seg type="syl" n="3">to</seg>
                    <seg type="syl" n="4">o-</seg>
                    <seg type="syl" n="5">pen</seg>
                    <seg type="syl" n="6">shame.</seg>
                </l>
            </lg>
            <lg type="verse" n="6-7">
                <l>
                    <seg type="syl" n="1">6. With</seg>
                    <seg type="syl" n="2">them</seg>
                    <seg type="syl" n="3">that</seg>
                    <seg type="syl" n="4">peace</seg>
                    <seg type="syl" n="5">did</seg>
                    <seg type="syl" n="6">hate</seg>
                </l>
                <l>
                    <seg type="syl" n="1">I</seg>
                    <seg type="syl" n="2">came</seg>
                    <seg type="syl" n="3">a</seg>
                    <seg type="syl" n="4">peace</seg>
                    <seg type="syl" n="5">to</seg>
                    <seg type="syl" n="6">make,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">And</seg>
                    <seg type="syl" n="2">set</seg>
                    <seg type="syl" n="3">a</seg>
                    <seg type="syl" n="4">qui-</seg>
                    <seg type="syl" n="5">et</seg>
                    <seg type="syl" n="6">life:</seg>
                </l>
                <l>
                    <seg type="syl" n="1">7. But</seg>
                    <seg type="syl" n="2">when</seg>
                    <seg type="syl" n="3">my</seg>
                    <seg type="syl" n="4">word</seg>
                    <seg type="syl" n="5">was</seg>
                    <seg type="syl" n="6">told,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">Cause-</seg>
                    <seg type="syl" n="2">less</seg>
                    <seg type="syl" n="3">I</seg>
                    <seg type="syl" n="4">was</seg>
                    <seg type="syl" n="5">con-</seg>
                    <seg type="syl" n="6">trolled,</seg>
                </l>
                <l>
                    <seg type="syl" n="1">By</seg>
                    <seg type="syl" n="2">them</seg>
                    <seg type="syl" n="3">that</seg>
                    <seg type="syl" n="4">would</seg>
                    <seg type="syl" n="5">have</seg>
                    <seg type="syl" n="6">strife.</seg>
                </l>
            </lg>
        </div>
    </body>
</text>
